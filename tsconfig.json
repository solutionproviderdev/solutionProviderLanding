{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "strict": true,
      "jsx": "react-jsx",
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": ".",  
      "paths": {
        "@/*": ["src/*"] 
      }
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }
  


